<div class='ps-5 pt-5'>
  <div class='mb-5 card'>
    {{#with navigation}}
      <div class='nav'>
        {{#each links}}
          <div class='nav-item'>
            <a href='{{href}}' class='nav-link'>{{text}}</a>
          </div>
        {{/each}}
        <a
          href='{{href}}'
          class='nav-link disabled'
        >Disabled</a>
      </div>
    {{/with}}
  </div>

</div>