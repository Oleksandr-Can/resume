<div class='ps-5 pt-5'>
  <div class='card'>
    {{#with navigation}}
      <div class='nav'>
        {{#each links}}
          {{#if href}}
            <div class='nav-item'>
              <a
                href='{{href}}'
                class='nav-link'
              >{{text}}</a>
            </div>
          {{else}}
            <a
              href='{{href}}'
              class='nav-link disabled'
            >{{text}}</a>
          {{/if}}
        {{/each}}
        {{!-- <a
          href='{{href}}'
          class='nav-link disabled'
        >Disabled</a> --}}
      </div>
    {{/with}}
  </div>

</div>