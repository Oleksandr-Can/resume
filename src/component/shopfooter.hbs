<div class='ps-5 pt-5'>
  <div class='row mt-5'>
    {{#each footer}}
      <div class='col-2 ms-0'>
        <div class='nav p-0 m-0'>
          {{#each this}}
            {{#if @last}}
              <p class='nav-item m-0 p-0'>
                <a
                  href='{{link}}'
                  class='nav-link disabled'
                >{{text}}</a>
              </p>
            {{else}}
              <p class='nav-item m-0 p-0'>
                <a
                  href='{{link}}'
                  class='nav-link'
                >{{text}}</a>
              </p>
            {{/if}}
          {{/each}}
        </div>
      </div>
    {{/each}}
  </div>
</div>