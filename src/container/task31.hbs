<main class='row g-2'>
  {{#with navigation}}
    {{#each links}}
      <div class='col-2'>
        <a href='{{href}}' class='btn btn-link'>{{text}}</a>
      </div>
    {{/each}}
    {{#with button}}
      <div class='col-2'>
        <a
          href='{{href}}'
          class='btn btn-primary btn-sm'
        >{{text}}</a>
      </div>
    {{/with}}
  {{/with}}
  <hr />

  <section>
    {{#with header}}
      <h1>{{title}}</h1>
      <p>{{description}}</p>
      {{#with button}}
        <a
          href='{{link}}'
          class='btn btn-primary'
        >{{text}}</a>
      {{/with}}
    {{/with}}
  </section>
  <hr />

  {{#with city}}
    <h1>{{title}}</h1>
    {{#each cards}}
      <section class='col-5'>
        <div class='card'>
          {{#with picture}}
            <img
              class='card-img-top'
              src='{{src}}'
              alt='{{alt}}'
            />
          {{/with}}
          <div class='card-body'>
            <h6 class='card-title'>{{title}}</h6>
            <p>{{description}}</p>
            {{#with button}}
              <a
                href='{{href}}'
                class='btn btn-primary'
              >{{text}}</a>
            {{/with}}
          </div>
        </div>
      </section>
    {{/each}}
  {{/with}}

  <hr />
  <section>
    {{#with winner}}
      <h2>{{title}}</h2>
      {{#with table}}
        <table class='table'>
          <thead>
            <tr>
              {{#each head}}
                <th>{{this}}</th>
              {{/each}}
            </tr>
          </thead>

          <tbody>
            {{#each user}}
              <tr>
                <td>
                  {{place}}
                </td>
                <td>
                  {{name}}
                </td>
                <td>
                  {{surname}}
                </td>
                <td>
                  {{nickname}}
                </td>
              </tr>
            {{/each}}

          </tbody>
        </table>
      {{/with}}
    {{/with}}
  </section>
  <hr />

  {{#with gallery}}
    <h2>{{title}}</h2>
    {{#each cards}}
      <section class='col-4'>
        <div class='card'>
          <div class='card-header'>{{name}}</div>
          <div class='card-body'>
            <ul class='list-group'>
              {{#each items}}
                <li class='list-group-item'>{{this}}</li>
              {{/each}}
            </ul>
          </div>
        </div>
      </section>
    {{/each}}
  {{/with}}
  <hr />

  {{#with relaxion}}
    <p class='display-3'>{{title}}</p>
    {{#each cards}}
      <div class='col-5'>
        <div class='card text-bg-secondary'>
          <div class='card-header'>{{header}}</div>
          <div class='card-body'>
            <h6 class='card-title'>{{title}}</h6>
            <p>{{description}}</p>
          </div>
        </div>
      </div>
    {{/each}}
  {{/with}}

  <hr />
  <dir class='row gx-2 gy-1'>
    {{#with rest}}
      {{#each cards}}
        <dev class='col-6'>
          <div class='card'>
            <div class='card-header text-center'>
              {{header}}</div>
            <div class='card-body text-center'>
              <h6 class='card-title'>{{title}}</h6>
              <p>{{description}}</p>
              <button
                class='btn btn-primary'
              >{{button}}</button>
            </div>
            <div class='card-footer text-center'>
              {{footer}}</div>
          </div>
        </dev>
      {{/each}}
    {{/with}}
  </dir>

  <hr />
  {{#with proposition}}
    <p class='display-2'>{{title}}</p>
    {{#each card}}
      <section class='col-5'>
        <div class='card text-bg-warning'>
          <div class='card-header'>{{header}}</div>
          <div class='card-body'>
            <h6 class='card-title'>{{title}}</h6>
            <p>{{description}}</p>
          </div>
        </div>
      </section>
    {{/each}}
  {{/with}}

  <hr />

  {{#with footer}}
    <h2>{{title}}</h2>
    <div class='col-3'>
      {{#each navigation}}
        <p>
          <a
            href='{{href}}'
            class='btn btn-link btn-sm'
          >{{text}}</a>
        </p>
      {{/each}}
    </div>
    <div class='col-3'>
      {{#each socialMedia}}
        <p>
          <a
            href='{{link}}'
            class='btn btn-outline-dark btn-sm'
          >{{name}}</a>
        </p>
      {{/each}}
    </div>
    <div class='col-3'>
      {{#each partners}}
        <p>
          <a
            href='{{link}}'
            class='btn btn-outline-info btn-sm'
          >{{name}}</a>
        </p>
      {{/each}}
    </div>
  {{/with}}

</main>